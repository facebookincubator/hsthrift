packages: */*.cabal
tests: true
-- IPV6 is disabled in the Github Actions environment,
-- so we make sure the appropriate tests in those packages
-- turn to IPV4.
package thrift-lib
  flags: +tests_use_ipv4
package thrift-server
  flags: +tests_use_ipv4
package thrift-cpp-channel
  flags: +tests_use_ipv4
